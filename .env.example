# Telegram Bot Configuration
# Get your bot token from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Dev User Configuration
# Comma-separated list of Telegram user IDs with full admin privileges
# To get your Telegram user ID, send /start to @userinfobot
# Example: DEV_USER_IDS=123456789,987654321
DEV_USER_IDS=your_telegram_user_id_here

# Bot Configuration
DEBUG=True